extends layout

block content
  h1= title
  p Welcome to #{title}
  p Lab 10 work of SobhaReddy Pittu 
  button(type="button",onclick="doBump()" class="btn btn-warning") Press Me

  div(id="bump") Bump the count
  script.
    var count=0;
    var bump=1;
    function doBump() {
      count = count+bump;
      bump++;
      document.getElementById("bump").innerHTML=`Count ${count} with bump ${bump}`
    }
    
  a(href="/university") go to university 